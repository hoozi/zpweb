    <div class="col-md-4"><span>实际抵港时间:</span>{{data.length > 1 ? data[1].dependActualTime : '-'}}</div>
    <div class="col-md-4"><span>预计离港时间:</span>{{data.length > 1 ? data[1].leavePlanTime : '-'}}</div>
    <div class="col-md-4"><span>实际离港时间:</span>{{data.length > 1 ? data[1].leaveActualTime : '-'}}</div>
  </div>
</div>
<div class="divider divider-horizontal"></div>
<div class="branch-item">
  <h2 class="branch-title">内支线靠港2</h2>
  <div class="row">
    <div class="col-md-4"><span>卸港2:</span>{{data[0].nzxDischargePort2}}</div>
    <div class="col-md-4"><span>装港2:</span>{{data[0].nzxLoadPort2}}</div>
    <div class="col-md-4"><span>预计抵港时间:</span>{{data.length > 2 ? data[2].dependPlanTime : '-'}}</div>
    <div class="col-md-4"><span>实际抵港时间:</span>{{data.length > 2 ? data[2].dependActualTime : '-'}}</div>
    <div class="col-md-4"><span>预计离港时间:</span>{{data.length > 2 ? data[2].leavePlanTime : '-'}}</div>
    <div class="col-md-4"><span>实际离港时间:</span>{{data.length > 2 ? data[2].leaveActualTime : '-'}}</div>
  </div>
</div>
<div class="divider divider-horizontal"></div>
<div class="branch-item">
  <h2 class="branch-title">内支线靠港3</h2>
  <div class="row">
    <div class="col-md-4"><span>卸港3:</span>{{data[0].nzxDischargePort3}}</div>
    <div class="col-md-4"><span>装港3:</span>{{data[0].nzxLoadPort3}}</div>
    <div class="col-md-4"><span>预计抵港时间:</span>{{data.length > 3 ? data[3].dependPlanTime : '-'}}</div>
    <div class="col-md-4"><span>实际抵港时间:</span>{{data.length > 3 ? data[3].dependActualTime : '-'}}</div>
    <div class="col-md-4"><span>预计离港时间:</span>{{data.length > 3 ? data[3].leavePlanTime : '-'}}</div>
    <div class="col-md-4"><span>实际离港时间:</span>{{data.length > 3 ? data[3].leaveActualTime : '-'}}</div>
  </div>
</div>
</script>


