<div class="layout-page-banner detail-banner">
  <div class="layout-page-banner-text">
    <h1>嘉港新闻</h1>
    <p>NEWS</p>
  </div>
</div>

<div class="page-body">
  <div class="news-tabs">
    <div class="news-tabs-nav">
      <span class="active tabs-nav__item" data-category="1">嘉港新闻</span>
      <span class="tabs-nav__item" data-category="2">通知公告</span>
      <span class="tabs-nav__item" data-category="3">行业资讯</span>
    </div>
    <div class="news-tabs-content container" id="news-tabs-content-area"></div>
  </div>
</div>
<script type="text/template" id="news-list--tpl">
<ul class="news-list">
{_.each(data, function(item){}
<li class="row">
  <a href="detail.html?id={{item.id}}" class="col-lg-9" title="{{item.title}}">{{item.title}}</a>
  <span class="col-lg-3 time">{{item.createDate?moment(item.createDate).format('YYYY-MM-DD'):'-'}}</span>
</li>
{})}
</ul>
</script>